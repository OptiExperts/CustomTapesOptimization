function OMGWaitGlobalVariable(){
  if (__st.p === 'product') {
      omgrfq_productCollection[__st. rid].properties = {};
      const checkRenderRadio = setInterval(() =>{ 
          if (OMGRFQGlobalVariables.$jQueryLib('.mw-option-radio .mw_options_list input').length) {
              OMGRFQGlobalVariables.$jQueryLib('.mw-option-radio .mw_options_list input').on('click', function() {
                  const dataName = OMGRFQGlobalVariables.$jQueryLib(this).closest('.mw-option-radio').find('.mw_product_option_label').first().text().replace("*", ""),
                  dataValue = OMGRFQGlobalVariables.$jQueryLib(this).val();
                  omgrfq_productCollection[__st.rid].properties[`${dataName}`] = dataValue;
                  setPriceNew()
              })
              clearInterval(checkRenderRadio);
          }
      })

const checkRenderColor = setInterval(() =>{ 
          if (OMGRFQGlobalVariables.$jQueryLib('.mw-option-swatch .swatch-option').length) {
              OMGRFQGlobalVariables.$jQueryLib('.swatch-option').on('click', function() {
                  const data = OMGRFQGlobalVariables.$jQueryLib(this).closest('.mw-option-swatch').find('.mw_product_option_label').first().text().replace("*", ""),
                  array = data .split("- "),
                  dataName = array[0],
                  dataValue = array[1];
                  omgrfq_productCollection[__st.rid].properties[`${dataName}`] = dataValue;
                  setPriceNew()
              })
              clearInterval(checkRenderColor);
          }
      })
  }

  function setPriceNew(){
    setTimeout(() => {
 const queryString = window.location.search
        const urlParams = new URLSearchParams(queryString)
        const variantId = urlParams.get('variant');
        let getMoneyPrice = OMGRFQGlobalVariables.$jQueryLib('.price__regular .productPrice').text().replace(',', '');
        getMoneyPrice = getMoneyPrice.match(/\d+(?:\.\d+)?/g)[0];
        const priceProductFirst = Number(getMoneyPrice);
        for (const variant of omgrfq_productCollection[__st.rid].variants) {
          if (variant.id == variantId) {
              variant.price = priceProductFirst * 100;
              console.log(variant.price)
          }
        }
    }, 300)
  }
}
